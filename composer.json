{
    "minumum-stability": "dev",
    "name": "haijin/tools",
    "description": "Common tools to use in PHP applications.",
    "keywords": ["utilities", "path", "collections"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/haijin-development/php-tools",
    "authors": [
        {
            "name": "Haijin Development",
            "email": "haijin.develpment@gmail.com",
            "homepage": "https://github.com/haijin-development",
            "role": "developer"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "repositories": {
        "haijin/specs": {
            "type": "path",
            "url": "../php-specs",
            "options": {
            "symlink": true
            }
        }
    },
    "require-dev": {
        "haijin/specs": "dev-development"
    },
    "autoload": {
        "psr-4": {
            "Haijin\\": ["src/collections", "src/path", "src/closure-context", "src/debugger", "src/files-cache"]
        }
    },
    "scripts": {
        "specs": "php ./vendor/bin/specs"
    }
}